package modulo5;

public class TesteConstrutor {

	public static void main(String[] args) {
		Pessoa p1 = new Pessoa();
		
		System.out.println("Nome: \"" + p1.nome + "\"tem" + p1.idade);
		
		p1.nome = "Joao";
		p1.idade = 25;
		
		System.out.println("Nome: \"" + p1.nome + "\"tem" + p1.idade);

	}

}
